{
  "name": "useful-optimizer-docs",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "docs:api": "cd .. && uv run griffe dump opt -d google -r -o docs/api/full_api.json && cd docs",
    "docs:api:all": "cd .. && uv run python scripts/generate_docs.py --all --json --griffe --full-api --sidebar && cd docs",
    "validate:migration": "node validate-docs-migration.js"
  },
  "devDependencies": {
    "@catppuccin/vitepress": "^0.1.2",
    "@types/node": "^20.10.0",
    "echarts": "^6.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "markdown-it-katex": "^2.0.3",
    "typescript": "^5.3.0",
    "vitepress": "^1.5.0",
    "vue": "^3.4.0",
    "vue-echarts": "^7.0.3"
  },
  "dependencies": {
    "@tresjs/cientos": "^4.3.1",
    "@tresjs/core": "^4.3.6",
    "d3": "^7.9.0",
    "d3-scale-chromatic": "^3.1.0",
    "three": "^0.170.0"
  }
}
